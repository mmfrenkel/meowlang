

Main()
{
	int jerrys_cookies = 0;
	class MOUSE Jerry;
	Jerry.Incr_Cookies();
	jerrys_cookies = Jerry.Get_Num_Cookies();
	free(Jerry);
	printf("%X\n", jerrys_cookies);
}

Class MOUSE {

	int cookies = 2;

	Incr_Cookies()
	{
	int existing_cookies = this.Get_Num_Cookies();
	cookies = existing_cookies + 1;
	}
	int Get_Num_Cookies()
	{
	return cookies;
	}

}
